.header_wrap{min-width: 1200px;margin: 0 auto;}
.header{width: 100%;}
.top_box{width: 1200px;margin: 0 auto;height: 35px;}
.header_wrap .header .top_box .header_left{width: 490px;height: 35px;line-height: 35px;float: left;margin-left: 17px;}
.header_wrap .header .top_box .header_left li{float: left;font-size: 12px;color: #999;}
.header_wrap .header .top_box .header_left ul li span{color: red;}
.header_right{width: 141px;height: 35px;padding-right: 10px;float: right;line-height: 35px;}
.header_right .header_ul li,.header_a{font-size: 12px;color: #999;float: left;}
.logo_wrap{width: 1200px;margin: 0 auto;height: 115px;}
.logo_wrap .logo{width: 192px;height: 49px;margin-left: 503px; padding-top: 30px;display: inline-block;}
.logo_wrap .logo img{width: 192px;height: 49px;}
.header_wrap .txt{float: right;position: relative;}
.header_wrap .txt .sub{list-style: none;display: block;width: 30px;height: 22px;background: url(http://image.masamaso.com/2016/seach.png) no-repeat 10px center;position: absolute;top: 1px;right: 122px;border: none;cursor: pointer;outline: none;}
.header_wrap .txt .seek{text-rendering: auto;color: initial;display: inline-block;width: 120px;height: 20px;border: 1px solid #959595;padding-left: 30px;text-align: left;}
.btn_bar_wrap{width: 100%;height: 30px;}
.btn_bar{width: 1200px;height: 30px;line-height: 30px;margin: 0 auto;}
.btn_bar .box1{width: 731px;height: 30px;float: left;}
.btn_bar .box1 li{height: 30px; padding: 0 15px;float: left; cursor: pointer;}
.btn_bar .box1 li a{font-size: 14px;position: relative;color: #666;}
.btn_bar li.top:hover{background: #900;}
.btn_bar>.box1>li>a:hover{text-decoration: none;color: #000;}
.btn_bar .box1 li a img{position: absolute;top: 5px;right: -15px;}
.btn_bar .box2{width: 335px;height: 30px;float: left;padding-left: 130px;}
.btn_bar .box2 li{padding:0 10px;height: 30px;float: left;cursor: pointer;}
.btn_bar .box2 li a{font-size: 14px;position: relative;color: #666;}
.btn_bar>.box2>li>a:hover a{text-decoration: none;color: #000;}
.btn_bar .box2 li a img{position: absolute;top: 5px;right: -15px;}
.header_nav_wrap{width: 100%;height: 28px;position: relative;}
.header_nav{width: 1200px;height: 28px;margin: 0 auto;line-height: 28px;overflow: hidden;}
.header_nav .nav_menu_tit li:hover{background: #333;}
.header_nav .nav_menu_tit{width: 840px; height: 28px;z-index: 10px;float: left;z-index: 10;}
.header_nav .nav_menu_tit li{text-align: center;padding-left: 25px;width: 66px;padding-left: 17px;}
.header_nav .nav_menu_tit2{width: 176px;height: 28px;float: right;z-index: 10;}
.header_nav .nav_menu_tit2 li{width: 67px;padding-left: 17px;}
.header_nav_wrap .header_nav .nav_menu_tit li.bbb{background: #333 !important;}
.header_nav_wrap .header_nav .nav_menu_tit2 li:hover{background: #333 !important;}
.header_nav .nav_menu_tit2 li a{font-size: 14px;color: #666;}
.header_nav li{float: left;width: 66px;cursor: pointer;padding-left: 17px;}
/*.header_nav li i{width: 10px;height: 9px; background: url(http://image.masamaso.com/2016/meau-arr.png?46165494) no-repeat center 0;}*/
.header_nav li a{font-size: 14px;color: #666;text-align: center;float: left;}
.header_nav li a:hover{color: #000;text-decoration: none;}
.header_nav_wrap .Scroll_wrap{z-index: 999;display: none; width: 1200px;height: 254px;background: #fff;float: left;position: absolute;top: 28px;}
.header_nav_wrap .nav_menu{float: left;width: 83px;height: 234px;margin-top: 20px;border-left: 1px solid #ccc;overflow: hidden;}
.header_nav_wrap .nav_menu li a{font-size: 12px;text-align: center;}
.header_nav_wrap .nav_menu li a:hover{text-decoration: underline;}
.header_nav_wrap .nav_menu img{float: left;}


.fixed_box{z-index: 999; width: 36px;position: fixed;background: rgb(30,30,30);height: 100%;right:0px;top:0px;}
.cart{cursor: pointer; width: 36px;height: 100px;padding: 15px 0;margin-top: 205px;text-align: center;position: relative;}
.cart:hover{background: #900;}
.cart:hover .quan{background: #fff;color: #000;}
.cart i{background: url(../img/nav_icon.png)no-repeat 0 0 ;width: 14px;height: 14px;margin: 0 0 3px 11px;float: left;}
.cart p{width: 20px;height: 50px;line-height: 16px;color: #fff;float: left;margin-left: 9px;}
.quan{width: 20px;height: 20px;border-radius: 10px;line-height: 20px;text-align: center;background: #900;float: left;font-size: 12px;color: #fff;margin-left: 10px;margin-top: 5px;}
.mycart{display: none; top: 40px; left: -130px;color: #fff;line-height: 33px;text-align: center; width: 123px;height: 33px;background: #900; /*animation: hide 0.4s ease both;*/position: absolute;}
.cart:hover .mycart{display: block;animation:  hide 0.4s ease both;}
.property_box{width: 36px;height: 50px;padding: 10px 0;position: relative;}
.property_box i{float: left;margin-left: 12px; background: url(../img/nav_icon.png)no-repeat -1px -42px ;width: 16px;height: 11px;float: left;}
.property_box p{font-size: 12px; width: 30px;height: 20px;line-height: 20px;color: #fff;float: left;margin-left: 9px;}
.myproperty{display: none; top: 23px; left: -130px;color: #fff;line-height: 33px;text-align: center; width: 123px;height: 33px;background: #900; animation: show 0.4s ease both;position: absolute;}
.property_box:hover .myproperty{display: block;animation: show 0.4s ease both;}
.property_box:hover{background: #900;}
.property_box:hover .quan{background: #fff;color: #000;}

.order{width: 36px;height: 38px;padding: 5px 0;margin: 5px 0 0;position: relative;cursor: pointer;}
.order i{float: left;margin-left: 12px; width: 10px;height: 14px;background: url(../img/nav_icon.png)no-repeat 0 -83px;}
.order p{width: 36px;height: 16px;color: #fff;font-size: 14px;text-align: center;float: left;}
.myorder{display: none; top: 5px; left: -130px;color: #fff;line-height: 33px;text-align: center; width: 123px;height: 33px;background: #900; animation: show 0.4s ease both;position: absolute;}
.order:hover .myorder{display: block;animation: show 0.4s ease both;}
.order:hover{background: #900;}



.collect{width: 36px;height: 38px;padding: 5px 0;margin: 5px 0 0;position: relative;cursor: pointer;}
.collect i{float: left;margin-left: 12px; width: 14px;height: 12px;background: url(../img/nav_icon.png)no-repeat -1px -118px;}
.collect p{width: 36px;height: 16px;color: #fff;font-size: 14px;text-align: center;float: left;}
.mycollect{display: none; top: 5px; left: -130px;color: #fff;line-height: 33px;text-align: center; width: 123px;height: 33px;background: #900; animation: show 0.4s ease both;position: absolute;}
.collect:hover .mycollect{display: block;animation: show 0.4s ease both;}
.collect:hover{background: #900;}

.fixed_box .phone{width: 36px;height: 50px;padding: 10px 0;margin: 5px 0 0;position: relative;cursor: pointer;}
.phone i{float: left;margin-left: 12px; width: 11px;height: 20px;background: url(../img/weixin2_icon.png)no-repeat 0 0;}
.phone p{width: 30px;height: 16px;color: #fff;font-size: 14px;float: left;}
.phone:hover{background: #900;}
.phone img{position: absolute;top: -315px;left: -170px;display: none;}
.phone:hover img{display: block;}
